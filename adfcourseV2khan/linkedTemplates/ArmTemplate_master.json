{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfcourseV2khan"
		},
		"airport_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'airport_sql_db'"
		},
		"datalakekhan_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakekhan'"
		},
		"on_prem_files_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'on_prem_files'"
		},
		"airport_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "airportdbadfv2.database.windows.net"
		},
		"airport_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airportdbV2"
		},
		"airport_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminkhan"
		},
		"datalakekhan_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakekhan.dfs.core.windows.net/"
		},
		"ls_git_csvs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"on_prem_files_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\ADFCourseFiles"
		},
		"on_prem_files_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "fardeenkhanbieng@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfcourseV2khan_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"airport_sql_db_password": {
						"value": "[parameters('airport_sql_db_password')]"
					},
					"datalakekhan_accountKey": {
						"value": "[parameters('datalakekhan_accountKey')]"
					},
					"on_prem_files_password": {
						"value": "[parameters('on_prem_files_password')]"
					},
					"airport_sql_db_properties_typeProperties_server": {
						"value": "[parameters('airport_sql_db_properties_typeProperties_server')]"
					},
					"airport_sql_db_properties_typeProperties_database": {
						"value": "[parameters('airport_sql_db_properties_typeProperties_database')]"
					},
					"airport_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('airport_sql_db_properties_typeProperties_userName')]"
					},
					"datalakekhan_properties_typeProperties_url": {
						"value": "[parameters('datalakekhan_properties_typeProperties_url')]"
					},
					"ls_git_csvs_properties_typeProperties_url": {
						"value": "[parameters('ls_git_csvs_properties_typeProperties_url')]"
					},
					"on_prem_files_properties_typeProperties_host": {
						"value": "[parameters('on_prem_files_properties_typeProperties_host')]"
					},
					"on_prem_files_properties_typeProperties_userId": {
						"value": "[parameters('on_prem_files_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfcourseV2khan_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfcourseV2khan_ArmTemplate_0')]"
			]
		}
	]
}